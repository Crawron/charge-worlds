{
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "prepare": "typesync",
    "dev": "node scripts/fonts.mjs && run-p --print-label --race dev:*",
    "dev:remix": "remix dev",
    "dev:tailwind": "pnpm run tailwind --watch",
    "build": "tsc --noEmit && pnpm run tailwind --minify && remix build && node scripts/fonts.mjs",
    "start": "cross-env NODE_ENV=production netlify dev",
    "lint": "eslint --ext js,ts,tsx,astro .",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "tailwind": "tailwindcss -o app/generated/tailwind.css -c tailwind.config.cjs"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.0",
    "@fontsource/oswald": "^4.5.10",
    "@fontsource/rubik": "^4.5.11",
    "@headlessui/react": "^1.7.2",
    "@liveblocks/client": "^0.18.2",
    "@liveblocks/node": "^0.18.2",
    "@liveblocks/react": "^0.18.2",
    "@nanostores/react": "^0.4.0",
    "@netlify/functions": "^1.2.0",
    "@remix-run/netlify": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@supabase/supabase-js": "2.0.0-rc.10",
    "ariakit": "2.0.0-next.41",
    "chalk": "^5.0.1",
    "clsx": "^1.2.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.2",
    "nanostores": "^0.7.0",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "react-feather": "^2.0.10",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "^1.1.5",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.21",
    "@types/node-cron": "^3.0.4",
    "@types/prettier": "^2.7.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "esbuild": "^0.15.9",
    "eslint": "^8.24.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "netlify-cli": "^11.8.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "typesync": "^0.9.2"
  },
  "prettier": "@itsmapleleaf/configs/prettier"
}
